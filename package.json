{
  "name": "@creoox-public/zitadel-client",
  "type": "module",
  "version": "0.52.4",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.9.0",
  "description": "ZITADEL custom js client",
  "author": "Creoox AG <info@creoox.com>",
  "license": "MIT",
  "homepage": "https://github.com/Creoox/zitadel-client#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Creoox/zitadel-client.git"
  },
  "bugs": "https://github.com/Creoox/zitadel-client/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "debug": "tsx src/debug.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "ky": "^1.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.3.2",
    "@antfu/ni": "^0.23.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.5.4",
    "bumpp": "^9.5.2",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.9.0",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vite": "^5.4.3",
    "vitest": "^2.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
